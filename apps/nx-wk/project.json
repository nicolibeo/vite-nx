{
  "root": "apps/nx-wk",
  "sourceRoot": "apps/nx-wk/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/nx-wk",
        "index": "apps/nx-wk/src/index.html",
        "baseHref": "/",
        "main": "apps/nx-wk/src/main.tsx",
        "polyfills": "apps/nx-wk/src/polyfills.ts",
        "tsConfig": "apps/nx-wk/tsconfig.app.json",
        "assets": ["apps/nx-wk/src/favicon.ico", "apps/nx-wk/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/nx-wk/src/environments/environment.ts",
              "with": "apps/nx-wk/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "nx-plugin-vite:serve",
      "options": {
        "configFile": "apps/nx-wk/tsconfig.json",
        "port": 3000,
        "host": false,
        "https": false
      }
    }
  },
  "tags": []
}
